; Micro Isolate ANntibiotic
DECLARE	1011_A_ISOLATE = f8			WITH PERSISTSCRIPT, CONSTANT (uar_get_code_by ("DISPLAYKEY", 1011, "A ISOLATE"))
DECLARE	1011_CVO_A_ISOLATE = vc		WITH PERSISTSCRIPT, NOCONSTANT (" ")
	SELECT	INTO "NL:"
	FROM	CODE_VALUE_OUTBOUND CVO
	 WHERE	CVO.code_value = 1011_A_ISOLATE
	  AND	CVO.contributor_source_cd = 73_EPIC
	DETAIL	1011_CVO_A_ISOLATE = trim (CVO.alias, 3)
	WITH	NOCOUNTER
 
